{
  "parser": "babel-eslint",
  "plugins": [
    "react"
  ],
  "globals": {
    "document": true,
    "window": true,
    "localStorage": true,
    "navigator": true,
    "self": true,
    "clients": true,
    "location": true,
    "fetch": true,
    "store": true,
    "__DEV__": true
  },
  "extends": "airbnb",
  "rules": {
    "arrow-parens": 0,
    "class-methods-use-this": 0,
    "arrow-body-style": 0,
    "prefer-template": 0,
    "max-len": 0,
    "indent": ["error", 2, { "ignoredNodes": ["JSXElement *"] }],
    "consistent-return": 0,
    "no-underscore-dangle": 0,
    "no-param-reassign": 0,
    "array-callback-return": 0,
    "linebreak-style": 0,
    "react/self-closing-comp": ["error", {
      "component": true,
      "html": false
    }],
    "react/jsx-closing-bracket-location": [1, "after-props"],
    "react/jsx-filename-extension": 0,
    "no-restricted-syntax": 0,
    "import/prefer-default-export": 0,
    "import/extensions": 0,
    "jsx-a11y/href-no-hash": 0,
    "react/sort-comp": 0
  },
  "settings": {
    "import/ignore": "node_modules",
    "import/resolver": {
      "node": {
        "extensions": [
          ".js",
          ".android.js",
          ".ios.js"
        ]
      }
    }
  }
}
